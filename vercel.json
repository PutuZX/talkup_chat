{
    "builds": [
        {
            "src": "/api/predict.js",
            "use": "@vercel/node"
        },
        {
            "src": "/src/app/nlp/storyNLP.py",
            "use": "@vercel/python",
            "config": { "maxLambdaSize": "50mb" }
        }
    ],
    "routes": [
        {
            "src": "/api/predict",
            "dest": "/api/predict.js"
        }
    ]
}
